    "@storybook/test": "^8.6.12",
    "@storybook/vue3": "^8.6.12",
    "@storybook/vue3-vite": "^8.6.12",
    "@types/node": "^22.15.17",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitest/browser": "^3.1.3",
    "@vitest/coverage-v8": "^3.1.3",
    "@vue/tsconfig": "^0.7.0",
    "husky": "^9.1.7",
    "playwright": "^1.52.0",
    "storybook": "^8.6.12",
    "typescript": "~5.8.3",
    "vite": "^6.3.5",
    "vitest": "^3.1.3",
    "vue-tsc": "^2.2.8"
